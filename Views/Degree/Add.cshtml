@model Degree

<form asp-action="Add" asp-controller="Degree">
    <input asp-for="Index" placeholder="Index"/>
    <input asp-for="Grade" type="number" step="0.5" max="5" min="0" placeholder="Grade"/>
    <input asp-for="Subject" placeholder="Subjecy"/>
    <input asp-for="AvregeGrade" hidden />
    <input type="submit" />
</form>
